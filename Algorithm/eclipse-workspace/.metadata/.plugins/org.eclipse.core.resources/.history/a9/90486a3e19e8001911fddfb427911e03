package kr.bang.algorithm;

import java.util.Arrays;
import java.util.Comparator;
import java.util.Scanner;
public class Comp implements Comparator<Integer>{
	public static int compare(int[] a, int[] b) {
		if(a[2]<b[2]) return 1;
		else if(a[2]==b[2]) return 0;
		else return -1;
	}	
}
public class Greedy2 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		int N = s.nextInt();
		int[][] conference = new int[N][3];
		for(int i=0; i<N; i++) {
			conference[i][2] = s.nextInt();
			conference[i][1] = s.nextInt();
			conference[i][0] = s.nextInt();
		}
		Arrays.sort(conference);
		for(int[] e : conference) {
			System.out.println(e[0]);
		}
	}

}
