package kr.bang.stepup;

import java.util.Scanner;

public class NumberBaseball {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int N = s.nextLine().charAt(0);
		String[] question = new String[N];
		/*
		 * 재귀함수를 써서 해보자. n번째 질문의 조건을 성립하면 n+1번의 조건으로 다시 재귀함수를 호출한다.
		 * 재귀함수의 인자로는 index, 조건을 전부 만족했는지 체크하기 위한 correct boolean,
		 * 질문들을 검증할 세자리 String, 그리고 입력받은 질문들의 String 배열이 필요하다.
		 */ 
		for(int i=0; i<N; i++) {
			question[i] = s.nextLine();
		}
		System.out.println();
		for(int i=0; i<N; i++) {
			System.out.println(i+" : "+question[i]);
		}
	}
}