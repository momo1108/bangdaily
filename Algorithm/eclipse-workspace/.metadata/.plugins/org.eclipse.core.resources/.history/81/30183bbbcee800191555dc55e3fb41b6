package kr.bang.algorithm;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class Jangi {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		Queue<int[]> queue = new LinkedList<int[]>();//큐 생성
		int N, M;
		N = s.nextInt();
		M = s.nextInt();
		int[] current = new int[3];
		current[0] = s.nextInt();
		current[1] = s.nextInt();
		current[2] = 0;		
		int destination = (s.nextInt()-1)*M + s.nextInt();
		queue.offer(new int[]{current, 0});
		System.out.println(current);
		System.out.println(destination);
		int[] wait = new int[2];
		int location, count;
		System.out.printf("%d, %d",location,count);
		// 함수는 똑같이 동작하도록 만들어놓고 위치와 실행 횟수를 같이 저장하는 큐를 만들자.
		while(true) {
			wait = queue.poll();
			location = wait[0];
			count = wait[1];
			if(location-2*M+1>0)
		}
		
	}
	
}
