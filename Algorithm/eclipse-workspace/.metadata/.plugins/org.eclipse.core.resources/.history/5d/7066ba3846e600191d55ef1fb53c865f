public class Test {

	public static void main(String[] args) {
		System.out.println((int)'2');
		// TODO Auto-generated method stub
		int n=10; // 1. 선언문
		n=n+5; // 2. 대입문
		System.out.println("안녕하세요~");
		mmfwefafewfwafewaf(); // 3. method 호출, 자동완성 - Ctrl+Space
		System.out.println("헬로 월드~"); // 자바에서의 콘솔(표준 입출력 장치 - 키보드, 모니터) 출력 - method 호출 - System class의 class가 저장된 변수 안의 method 호출
	} // end main
	public static void mmfwefafewfwafewaf() {
		System.out.println("mm 메소드입니다~"); // 한 줄 복사 - Ctrl+Alt+↓)
		System.out.println("mm 메소드입니다~"); // 커서 위치 줄 삭제 - Ctrl+D
		System.out.println("mm 메소드입니다~"); // 선택된 줄 이동 - Alt+↓
	}
}