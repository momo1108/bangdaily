package kr.bang.test;

import java.util.Scanner;

public class Array2_5 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		int[][][] arr = new int[3][3][5];
		for(int i=0; i<3; i++) {
			if(i==0) {
				System.out.println("first array");
				for(int j=0; j<3; j++) {
					for(int k=0; k<5; k++) {
						arr[i][j][k] = s.nextInt();
					}
				}
			}
			else if(i==1) {
				System.out.println("second array");
				for(int j=0; j<3; j++) {
					for(int k=0; k<5; k++) {
						arr[i][j][k] = s.nextInt();
					}
				}
			} else {
				for(int j=0; j<3; j++) {
					for(int k=0; k<5; k++) {
						arr[2][j][k] = arr[0][j][k]*arr[1][j][k];
					}
				}
			}
			
		}
	}

}
